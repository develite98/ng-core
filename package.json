{
  "name": "hulk",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "ng": "nx",
    "preinstall": "node ./scripts/preinstall.js",
    "postinstall": "husky install && node ./scripts/postinstall.js && node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "build:all": "nx run-many --target build --all --parallel --with-deps",
    "test": "nx test",
    "test:all": "nx run-many --target test --all --parallel --code-coverage",
    "lint": "nx workspace-lint && nx lint",
    "lint:all": "nx run-many --target lint --all --parallel",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:build:single-spa": "nx affected --target build-spa",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "commit": "git-cz"
  },
  "dependencies": {
    "@agm/core": "3.0.0-beta.0",
    "@angular/animations": "11.2.13",
    "@angular/cdk": "11.2.13",
    "@angular/common": "11.2.13",
    "@angular/compiler": "11.2.13",
    "@angular/core": "11.2.13",
    "@angular/forms": "11.2.13",
    "@angular/platform-browser": "11.2.13",
    "@angular/platform-browser-dynamic": "11.2.13",
    "@angular/router": "11.2.13",
    "@ngneat/error-tailor": "1.4.3",
    "@ngrx/component-store": "11.1.1",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "lodash-es": "4.17.4",
    "ng-zorro-antd": "11.4.1",
    "ngx-quill": "14.0.0",
    "quill": "1.3.7",
    "rxjs": "6.6.7",
    "single-spa": "5.9.3",
    "single-spa-angular": "4.9.2",
    "tslib": "2.2.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.1.1",
    "@angular-devkit/build-angular": "0.1102.13",
    "@angular-eslint/eslint-plugin": "4.3.0",
    "@angular-eslint/eslint-plugin-template": "4.3.0",
    "@angular-eslint/template-parser": "4.3.0",
    "@angular/cli": "11.2.13",
    "@angular/compiler-cli": "11.2.13",
    "@angular/language-service": "11.2.13",
    "@eztool/prettier-plugin-formatter": "1.0.1",
    "@nrwl/angular": "12.2.0",
    "@nrwl/cli": "12.2.0",
    "@nrwl/cypress": "12.2.0",
    "@nrwl/eslint-plugin-nx": "12.2.0",
    "@nrwl/jest": "12.2.0",
    "@nrwl/linter": "12.2.0",
    "@nrwl/nx-cloud": "12.1.1",
    "@nrwl/tao": "12.2.0",
    "@nrwl/workspace": "12.2.0",
    "@types/jest": "26.0.23",
    "@types/googlemaps": "3.43.3",
    "@types/lodash-es": "4.17.4",
    "@types/node": "15.3.0",
    "@types/quill": "2.0.7",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "commitizen": "4.2.4",
    "cypress": "7.3.0",
    "cz-customizable": "6.3.0",
    "dotenv": "9.0.2",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.11.3",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "jest-preset-angular": "8.4.0",
    "lint-staged": "11.0.0",
    "ng-packagr": "11.0.0",
    "prettier": "2.3.0",
    "pretty-quick": "3.1.0",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "typescript": "4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz-config.js"
    }
  },
  "importSort": {
    ".js,.jsx,.ts,.tsx": {
      "parser": "typescript",
      "style": "eslint"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,scss}": [
      "pretty-quick --staged",
      "git add -f"
    ],
    "*.html": [
      "node ./scripts/html-formatter.js --staged",
      "git add -f"
    ]
  },
  "workspaces": [
    "tools/eslint"
  ]
}
